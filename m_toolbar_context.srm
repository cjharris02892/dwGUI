on m_toolbar_context.create
m_toolbar_context=this
call super::create
this.text = "m_toolbar_context"
this.menutitles = false
this.m_size=create m_size
this.m_0=create m_0
this.m_showtoolbartips=create m_showtoolbartips
this.m_showtoolbartext=create m_showtoolbartext
this.Item[UpperBound(this.Item)+1]=this.m_size
this.Item[UpperBound(this.Item)+1]=this.m_0
this.Item[UpperBound(this.Item)+1]=this.m_showtoolbartips
this.Item[UpperBound(this.Item)+1]=this.m_showtoolbartext
end on

on m_toolbar_context.destroy
call super::destroy
destroy(this.m_size)
destroy(this.m_0)
destroy(this.m_showtoolbartips)
destroy(this.m_showtoolbartext)
end on

